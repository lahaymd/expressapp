div(ng-controller='NavController as vm')
		md-nav-bar(md-selected-nav-item='vm.currentNavItem'  layout='row' layout-align='space-around' layout-fill layout-margin layout-padding) 
			//- md-nav-item( name='home' md-nav-click='goto("home")' ui-sref='home' ui-sref-active='active') Home
			md-nav-item( name='directive' md-nav-click="goto('directive')" ui-sref='directive' ui-sref-active='active') Directive
			md-nav-item( name='users' md-nav-click="goto('users')" ui-sref='users' ui-sref-active='active') Users
			md-nav-item( name='register' md-nav-click="goto('register')" ui-sref='register' ui-sref-active='active') Register
			md-nav-item( name='login' md-nav-click="goto('login')" ui-sref='login' ui-sref-active='active') Login
			md-nav-item( name='readerboard' md-nav-click="goto('readerboard')" ui-sref='readerboard' ui-sref-active='active') Readerboard
			md-button( ng-click='vm.open()' hide-gt-sm)
				md-icon(md-svg-src='/static/material-design-icons/navigation/svg/production/ic_menu_24px.svg')
			//- md-button(ng-click='ctrl.showAlert($event)' md-raised) open modal
		
