theme = pink

div.lost-wrapper 
  lost-utility clearfix
  margin 100px 50px 50px 50px
 
  width 100%
  
  
  div
    background theme
    margin-top 10px
    height 100px
    lost-column 1/1
    +between('l', 'xl')
      background #bada55
      lost-column 1/3
    //&:at-most(7)
      //background #222
    div
      lost-column 1/2
      height 50px
      background complement(theme)
    &:nth-child(3)
      background invert(theme)
      