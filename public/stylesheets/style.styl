.diff.ng-enter
	animation slideInRight 1s
	
.diff.ng-enter-stagger
	animation-delay 0.1s	
	animation-duration 0s
	-webkit-animation-delay 0.1s
	-webkit-animation-duration 0s
	transition-delay 0.1s
	transition-duration 0s
	-webkit-transition-delay 0.1s
	-webkit-transition-duration 0s
#stick
	position sticky
	top 100px
	z-index 1

.checkstyle
	color green
	
#tool-container
	position absolute
	height 200px
	width 400px

body
	height auto !important

.md-nav-bar
	width 100%
	
ul
	justify-content space-around

.loginBox
	max-width 90%
	width 500px



[ui-view].ng-enter
	animation slideInRight 1s
    


	
[ui-view].ng-leave
	animation slideOutRight 1s

	

.ui-view.ng-enter
	animation zoomIn 1s
	
.ui-view.ng-enter.ng-enter-active
	background red
	
.ui-view.ng-leave
	animation hinge 1s
	
.ui-view.ng-leave.ng-leave-active
	background blue
	float right
	
.repeatedItem.ng-leave
	animation bounceOut 3s

.repeatedItem.ng-enter
	animation bounceIn 3s
	

